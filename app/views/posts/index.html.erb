<p id="notice"><%= notice %></p>
<% @posts.each do |post| %>
  <h1><%= link_to post.title, post %></h1>
<% end %>

<br>

<% if user_signed_in? %>
<p><%= link_to 'New Post', new_post_path %></p>
<p><%= link_to 'Logout', destroy_user_session_path, method: :delete %></p>
<% else %>
<%= link_to 'Login', new_user_session_path %>
<% end %>
